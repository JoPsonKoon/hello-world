<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
<!-- generatorConfig.xml,文件的编码使用utf-8不行的话,使用gbk就ok -->
<!-- 设置mysql驱动路径 -->
<classPathEntry location="/Users/jopson/Documents/Manager/apache-maven/respository/mysql/mysql-connector-java/5.1.38/mysql-connector-java-5.1.38.jar" />
<!-- 此处指定生成针对MyBatis3的DAO -->
  <context id="context1"  targetRuntime="MyBatis3">
  <commentGenerator>  
    <!-- 是否去除自动生成的注释 true：是 ： false:否 -->  
    <property name="suppressAllComments" value="true" />
    <property name="suppressDate" value="true" />    
  </commentGenerator>   
   <!-- jdbc连接信息 -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"  
    connectionURL="jdbc:mysql://116.6.194.38:3306/o2o" 
    userId="root" password="hw123123" />
    
     <!--  默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer  
         true，把JDBC DECIMAL 和 NUMERIC 类型解析为java.math.BigDecimal  
     --> 
    <javaTypeResolver>
       <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>  
    
    <!-- 生成模型的包名和位置（报错就把位置改成项目的名称） -->
    <javaModelGenerator targetPackage="com.ruizhisoft.o2o.dmi.model" targetProject="O2O-DMIAPI/src/main/java" />
    <!-- 生成映射文件的包名和位置 -->
    <sqlMapGenerator targetPackage="com.ruizhisoft.o2o.dmi.mapping" targetProject="O2O-DMIIMPL/src/main/java" />
    <!-- 生成DAO的类文件以及配置文件 -->
    <javaClientGenerator targetPackage="com.ruizhisoft.o2o.dmi.dao" targetProject="O2O-DMIIMPL/src/main/java" type="XMLMAPPER" />
    <!-- 想要生成的数据库表,自动化工具会根据该表的结构生成相应的vo对象 -->
     <!-- tableName:用于自动生成代码的数据库表；domainObjectName:对应于数据库表的javaBean类名 --> 
    <table schema="" tableName="pmi_device_info" domainObjectName=""
    		enableCountByExample="false"  enableUpdateByExample="false"
            enableDeleteByExample="false" enableSelectByExample="false"
            selectByExampleQueryId="false" >
    </table>
  </context>
</generatorConfiguration>